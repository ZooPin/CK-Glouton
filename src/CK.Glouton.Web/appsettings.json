{
  "Logging": {
    "PathFormat": "Logs/CK.Glouton-{Date}.txt",
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Lucene": {
    "MaxSearch": "10",
    "Path": "%localappdata%/Glouton/Logs",
    "Directory": ""
  } 
}
